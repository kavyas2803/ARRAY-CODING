#include <stdio.h>
int main()
{
    int n,i,arr[100],d;
    printf("\n Enter the num of elements in an array");
    scanf("%d",&n);
    printf("\n Enter the array elements");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("\n enter the index from which it should be rotate");
    scanf("%d",&d);
    for(int i=d;i<n;i++)
    {
        printf("%d",arr[i]);
    }
    for(i=0;i<d;i++)
    {
        printf("%d",arr[i]);
    }
    return 0;
}
